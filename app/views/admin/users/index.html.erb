<h2><%= link_to "Add user", new_admin_user_path %></h2>
<% @users.each do |user| %>
  <h3><%= user.full_name %></h3> (<%= link_to "edit", edit_admin_user_path(user) %>)(<%= link_to "delete", admin_user_path(user), method: :delete, confirm: "You sure?"%>)
  <h3><%= user.email %></h3>
  <h3><%= user.admin %></h3>
  <hr>
<% end %>